setUserProfile(response.data));\r\n        })\r\n    }\r\n}\r\n\r\nexport const getUserStatus = (userId) => {\r\n    return (dispatch) => {\r\n        profileAPI.getStatus(userId)\r\n            .then(response => {\r\n            dispatch(setStatus(response.data));\r\n        })\r\n    }\r\n}\r\n\r\nexport const updateStatus = (status) => {\r\n    return (dispatch) => {\r\n        profileAPI.updateStatus(status)\r\n            .then(response => {\r\n                if (response.data.resultCode === 0) {\r\n                    dispatch(setStatus(status));\r\n                }\r\n            })\r\n    }\r\n}\r\n\r\nexport default profileReducer;","D:\\programer\\it-kamasutra\\react_project\\src\\components\\Navbar\\Navbar.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\News\\News.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Music\\Music.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Dialogs\\DialogsContainer.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Settings\\Settings.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Users\\UsersContainer.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Header\\HeaderContainer.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Profile\\ProfileContainer.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Login\\Login.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\api\\api.js",[],"D:\\programer\\it-kamasutra\\react_project\\src\\hoc\\WithAuthRedirect.js",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Dialogs\\Dialogs.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Users\\Users.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Header\\Header.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\Profile\\Profile.jsx",[],"D:\\programer\\it-kamasutra\\react_project\\src\\components\\